{
  "name": "payload-bidirectional-relationships-plugin",
  "version": "0.4.0-v3",
  "description": "plugin for Payload CMS, which enables bidirectional relationships",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "WALX Tech e.K.",
    "email": "x@walx.tech",
    "url": "https://walx.tech"
  },
  "contributors": [
    {
      "name": "ле41%",
      "email": "_@w41x.io",
      "url": "https://w41x.io"
    }
  ],
  "homepage": "https://github.com/w41x/payload-bidirectional-relationships-plugin#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/w41x/payload-bidirectional-relationships-plugin.git"
  },
  "keywords": [
    "payload",
    "cms",
    "plugin",
    "bidirectional",
    "relationships"
  ],
  "bugs": {
    "url": "https://github.com/w41x/payload-bidirectional-relationships-plugin/issues"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm build:swc && pnpm build:types",
    "build:swc": "swc src -d dist --config-file .swcrc --strip-leading-paths",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "clean": "rimraf {dist, tsconfig.tsbuildinfo}",
    "prepublishOnly": "pnpm clean && pnpm turbo build"
  },
  "dependencies": {
    "payload": "3.0.0-beta.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.4.13",
    "@types/node": "^20.12.7",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.2",
    "turbo": "^1.13.2",
    "typescript": "^5.4.4"
  },
  "engines": {
    "node": "^21.7.2"
  },
  "packageManager": "pnpm@8.15.6",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "require": "./dist/types.js",
      "types": "./dist/types.d.ts"
    }
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "require": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "./types": {
        "import": "./dist/types.js",
        "require": "./dist/types.js",
        "types": "./dist/types.d.ts"
      }
    },
    "main": "./dist/index.js",
    "registry": "https://registry.npmjs.org",
    "types": "./dist/index.d.ts"
  }
}
