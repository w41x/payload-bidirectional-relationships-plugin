{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "clean": {
      "cache": false,
      "dependsOn": [
        "^clean"
      ]
    },
    "build": {
      "cache": true,
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "./dist/**"
      ]
    },
    "dev": {
      "cache": false
    },
    "lint": {
      "cache": false,
      "dependsOn": [
        "^lint"
      ]
    }
  }
}